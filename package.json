  "release": {
    "analyzeCommits": "conventional"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "resolutions": {
    "@graasp/sdk": "1.1.3"
  },
  "packageManager": "yarn@3.2.4",
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.{js,ts,jsx,tsx}"
