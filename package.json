    "prettier": "~2.8.6",
    "pretty-quick": "~3.1.3",
    "script-ext-html-webpack-plugin": "~2.1.5",
    "ts-loader": "9.4.2",
    "uglifyjs-webpack-plugin": "~2.2.0",
    "wait-on": "7.0.1",
    "webpack-cli": "~5.0.1"
  },
  "release": {
    "analyzeCommits": "conventional"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "packageManager": "yarn@3.2.4",
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.{js,ts,jsx,tsx}"
    ],
    "exclude": [
      "src/setupTests.ts",
      "src/**/*.d.ts",
      "cypress/*"
    ]
  }
